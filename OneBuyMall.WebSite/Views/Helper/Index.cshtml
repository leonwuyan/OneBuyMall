@model OneBuyMall.Models.Helper
<!DOCTYPE html>
<html>
<head>
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <title>帮助中心</title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link href="@Url.Content("~/style/css/main.css")" rel="stylesheet" type="text/css" />
